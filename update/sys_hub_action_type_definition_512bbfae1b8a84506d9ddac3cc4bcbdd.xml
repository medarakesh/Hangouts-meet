        <value>(function execute(inputs, outputs) {
    var data = [],
        results = {};
    data.push({
        "label": "Use Session Time Zone",
        "name": 'NULL_OVERRIDE'
    });
    var timeZones = new GlideRecord('sys_choice');
    timeZones.addEncodedQuery("elementSTARTSWITHtime_zone^inactive=false^nameSTARTSWITHsys_user");
    timeZones.orderBy("label");
    timeZones.query();
    while (timeZones.next()) {
        if (timeZones.getValue('value') == 'NULL_OVERRIDE')
            continue;
        data.push({
            "label": timeZones.getValue('label'),
            "name": timeZones.getValue('value')
        });
    }
    results.data = data;
    outputs.time_zones = results;
})(inputs, outputs);</value>
