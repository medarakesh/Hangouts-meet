        <name>GoogleHangoutsMeetUtil</name>
        <script><![CDATA[var GoogleHangoutsMeetUtil = Class.create();
GoogleHangoutsMeetUtil.prototype = {
    initialize: function() {
        this.EMAIL_REGEX = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
    },
    isValidEmailAddress: function(email) {
        return this.EMAIL_REGEX.test(email);
    },

    type: 'GoogleHangoutsMeetUtil'
};]]></script>
